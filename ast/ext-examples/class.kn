; XCLASS fraction {
	FIELD numer
	FIELD denom

	METHOD constructor (numer denom) {
		; .= this numer numer
		; & (? denom 0)
			; OUTPUT "zero denominator!"
			: QUIT 1
		: .= this denom denom
	}

	METHOD to_string () {
		+++ "" (. this numer) "/" (. this denom)
	}
}

; = frac XNEW fraction X[ 5 2 X]
: OUTPUT frac # => 5/2